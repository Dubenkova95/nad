public class DZ {

	
	public static void main(String[] args) {
	int a = 3;
    int b = 1;
    int c = 5;
    if (a >= b) {
    	 System.out.println(a);
    }
    else {
    	System.out.println(b);
    }
    if (b >= c) {
   	 System.out.println(b);
   }
   
    else 
    {
    	System.out.println(c);
    }
    if (a >=c) {
    	System.out.println(a);
    }

    else  {
    	System.out.println(c);
	}
	
	}
}

